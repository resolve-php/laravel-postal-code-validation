{
    "name": "resolve/laravel-postal-code-validation",
    "description": "Sensible postal code validation rules for countries, states, and provinces",
    "type": "library",
    "keywords": ["laravel", "validation", "postal code", "zip code"],
    "license": "MIT",
    "authors": [
        {
            "name": "Choraimy Kroonstuiver",
            "homepage": "https://github.com/axlon"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/validation": "^10.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "orchestra/testbench": "^8.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^10.5",
        "slevomat/coding-standard": "^8.14",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Resolve\\Validation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
